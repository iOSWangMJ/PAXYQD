page{
  padding: 22rpx 30rpx 72rpx 30rpx;
}
.rt{
  float: right;
}
.header{
  min-height: 56rpx;
  padding: 22rpx 20rpx 24rpx;
  display: flex;
  justify-content: center;
}
.header,.dynamic-area{
  background: white;
  border: 4rpx solid black;
  box-sizing: border-box;
  border-radius: 20rpx;
}
.auth{
  text-align: center;
  display: inline-block;
}
.fahongbao,.daishouqian,.pairenwu,.duixian,.fenxiang,.taohongbao{
  width: 34rpx;
  height: 34rpx;
  vertical-align: middle;
}
.auth-text{
  margin-left: 8rpx;
  margin-right: 8rpx;
  font-size: 26rpx;
  color: #333;
}
.personal{
  display: inline-block;
}
.dynamic-area,.header,.banner{
  margin-bottom: 16rpx;
}
.banner{  
  width: 100%;
  height: 290rpx; 
  background-size: 100% 100%;
}
.nail{
  position: absolute;
  width: 28rpx;
  height: 68rpx;
  margin-top: -30rpx;
}
.lf-img{
  left: 126rpx;
}
.rt-img{
  right: 126rpx;
}
.dynamic-content{
  position: relative;
  padding: 58rpx 0 30rpx 30rpx;
}
.dynamic-content{
  overflow: hidden;
}
.lfsection,.rtsection{
  float: left;
}
.rtsection{
  margin-left: 14rpx;
  margin-top: 8rpx;
  width: 579rpx;
}
.personal-icon{
  border-radius: 50%;
  box-sizing: border-box;
  border: 4rpx solid black;
  padding: 10rpx;
  width: 60rpx;
  height: 60rpx;
}
.member-tag{
  font-size: 32rpx;
  color: #333;
}
.activity-status{
  margin-left: 36rpx;
}
.tag-style{
  font-size: 24rpx;
  padding: 4rpx 10rpx;
  border-radius: 40rpx;
  position: relative;
  bottom: 2rpx;
}
.invite-parent{
  background: #0032c3;
  color: #fff;
}
.invite-baby{
  background: #1ed015;
  color: #fff;
}
.invite-friend{
  background: #cd0a21;
  color: #fff;
}
.time{
  font-size: 22rpx;
  color: #636363;
}
/*.time{
  margin-left: 10rpx;
}*/
.apply-finish{
  color: white;
  background: #fed015;
}
.activity-title{
  margin-bottom: 16rpx;
}
.activity-desc,.activity-icon{
  width: 469rpx;
}
.thumbnail{
  width: 100rpx;
  height: 100rpx;
  margin-right: 25rpx;
}
.activity-desc{
  font-size: 26rpx;
  color: #333;
  word-break: break-all;
  line-height: 36rpx;
}
.activity-icon{
  margin-top: 16rpx;
}
.thumbnail{
  width: 100rpx;
  height: 100rpx;
}
.askfor-redpacket{
  background: #fa510d;
  color: white;
}
.award{
  float: right;
  width: 120rpx;
  height: 50rpx;
  background: url("http://paicwx.oss-cn-shanghai.aliyuncs.com/btn@2x.png");
  background-size: 100% 100%;
  margin-right: 30rpx;
  margin-top: -5rpx;
  background-color: transparent;
}
.award::after{
  border: none;
}
.hongbao{
  position: absolute;
  right: 0;
  font-size: 22rpx;
  color: white;
  height: 60rpx;
  line-height: 60rpx;
}
.reward{
  background: url("http://paicwx.oss-cn-shanghai.aliyuncs.com/jiang_bg@2x.png");
  background-size: 200% 100%;
  padding: 18rpx 19rpx;
}
.achieve{
  background: url("http://paicwx.oss-cn-shanghai.aliyuncs.com/shouqian_bg@2x.png");
  background-size: 200% 100%;
  padding: 18rpx 19rpx;
}
.redpacket{
  background: url("http://paicwx.oss-cn-shanghai.aliyuncs.com/honbao_bg@2x.png");
  background-size: 200% 100%;
  padding: 18rpx 8rpx;
}
.askfor-activity-icon{
  min-height: 50rpx;
}
.reward-sum{
  background: url("http://paicwx.oss-cn-shanghai.aliyuncs.com/jiang_bg@2x.png") 100%;
}
.redpacket-sum{
  background: url("http://paicwx.oss-cn-shanghai.aliyuncs.com/honbao_bg@2x.png") 100%;
}
.achieve-sum{
  background: url("http://paicwx.oss-cn-shanghai.aliyuncs.com/shouqian_bg@2x.png") 100%;
}
.reward-sum,.redpacket-sum,.achieve-sum{
  background-size: 200% 100%;
  padding: 18rpx 20rpx 18rpx 10rpx;
}
.line{
