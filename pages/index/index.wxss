}
.arrow{
  position: relative;
  top: -3rpx;
  width: 28rpx;
  height: 14rpx;
}
.tast-figure{
  margin-right: 30rpx;
  margin-top: -10rpx;
}
.sym{
  font-size: 40rpx;
  color: #666;
}
.unit{
  font-size: 22rpx;
  color: #666;
}
.money{
  position: relative;
  top: 4rpx;
  padding: 0 10rpx;
  font-size: 40rpx;
  color: #D8232A;
}
.activity-footer{
  line-height: 24rpx;
}
.complete-date{
